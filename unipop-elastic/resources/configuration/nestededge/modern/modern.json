{
  "class": "org.unipop.elastic.ElasticSourceProvider",
  "clusterName": "unipop",
  "addresses": [
    "http://localhost:9200"
  ],
  "vertices": [
    {
      "index": "vertex",
      "type": "animal",
      "id": "@_id",
      "label": "animal",
      "dynamicProperties": {
        "excludeFields":[
          "_type"
        ]
      }
    },
    {
      "index": "vertex",
      "type": "person",
      "id": "@_id",
      "label": "person",
      "dynamicProperties": {
        "excludeFields":[
          "_type",
          "creations"
        ]
      },
      "edges": [
        {
          "path": "creations",
          "id": "@creationId",
          "label": "created",
          "direction": "OUT",
          "properties": {
            "weight": "@weight",
            "name": "@name"
          },
          "vertex": {
            "ref": false,
            "id": "@softwareId",
            "label": "software",
            "properties": {
              "name": "@name",
              "lang": "@lang"
            }
          }
        }
      ]
    }
  ],
  "edges": [
    {
      "index": "edge",
      "id": "@_id",
      "label": {
        "field": "_type",
        "exclude": [
          "created"
        ]
      },
      "properties": {},
      "dynamicProperties": {
        "excludeFields": [
          "outId",
          "inId",
          "outLabel",
          "inLabel"
        ]
      },
      "outVertex": {
        "ref": true,
        "id": "@outId",
        "label": "@outLabel",
        "properties": {}
      },
      "inVertex": {
        "ref": true,
        "id": "@inId",
        "label": "@inLabel",
        "properties": {}
      }
    }
  ]
}